{
  "settings": {
    "loss": "mse",
    "metrics": ["mae"],
    "optimizer": "Adam",
    "epochs": 20
  },
  "evaluation_protocol": {
    "mode": "expanding",
    "folds": 5,
    "train_size": 0.7,
    "val_size": 0.2,
    "test_size": 0.1
  },
  "window": {
    "input_width": 1,
    "label_width": 1,
    "shift": 1
  },
  "model": {
    "name": "fc",
    "params": {
      "units_per_hidden_layer": [
        3,5
      ],
      "regularization": {
        "l1": "1e-3",
        "l2": "0",
        "dropout": 0
      },
      "activation": "linear",
      "task": "regression"
    }
  }
}